
@{
    ViewBag.Title = "Fish";
}

<div id="category">
	<h2>Edit @ViewBag.Title</h2>
	<p>Here you can add a @ViewBag.Title to your place</p>
	<form action="/Settings/Setting/AddRiverFish" method="post" enctype="multipart/form-data">
		<div class="row">
			<div class="col-md-4">
				<div class="form-group">
					<label for="Name">Name</label>
					<input type="text" name="Name" required />
				</div>
			</div>
			<div class="col-md-4">
				<div class="form-group">
					<label for="Weight">Weight (Kg)</label>
					<input type="text" name="Weight" />
				</div>
			</div>
			<div class="col-md-4">
				<div class="form-group">
					<label for="Length">Length (M)</label>
					<input type="text" name="Length" />
				</div>
			</div>
			<div class="col-md-12">
				<input type="submit" value="Save" class="btn btn-outline-info" />
				<a href="javascript:void(0);" onclick="history.go(-1);" class="btn btn-outline-info">Go Back</a>
			</div>
		</div>
	</form>
</div>


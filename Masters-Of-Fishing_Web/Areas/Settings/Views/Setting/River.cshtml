
@{
    ViewBag.Title = "River";
}

<div id="category">
	<div class="row">
		<div class="col-md-12">
			<h2>@ViewBag.Title</h2>
			<p>Add, edit or delete a fish from this category</p>
		</div>
		<div class="col-md-12">
			<a href="" class="addButton">Add Fish</a>
		</div>
		<div class="col-md-12">
			<table class="CMSTable">
				<tr>
					<th>ID</th>
					<th>Name</th>
					<th>Action</th>
				</tr>
				<tr>
					<td>ID</td>
					<td>Title</td>
					<td>
						<a href="/Settings/Setting/EditFish"><i class="fas fa-pen"></i></a>
						<a href="" onclick="return confirm('Are you sure?')"><i class="fas fa-trash"></i></a>
					</td>
				</tr>
			</table>
		</div>
	</div>
</div>
<div id="footer">
	<a href="javascript:void(0);" onclick="history.go(-1);" class="menuButton">< Back to Settings</a>
</div>


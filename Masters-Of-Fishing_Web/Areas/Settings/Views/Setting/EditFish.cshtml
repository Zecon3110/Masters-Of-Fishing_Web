
@{
    ViewBag.Title = "Fish";
}

<div id="category">
	<h2>Edit @ViewBag.Title</h2>
	<p>Here you can edit your @ViewBag.Title</p>
	<form action="/Settings/Setting/EditFish" method="post" enctype="multipart/form-data">
		<input type="hidden" name="ID" value="" />
		<div class="row">
			<div class="col-md" -6>
				<div class="form-group">
					<label for="Name">Name</label>
					<input type="text" name="Name" placeholder="Name" value="" required />
				</div>
			</div>
			<div class="col-md" -6>
				<div class="form-group">
					<label for="Test">Test</label>
					<input type="text" name="Test" placeholder="Test" value="" required />
				</div>
			</div>
			<div class="col-md-12">
				<input type="submit" value="Save" class="btn btn-outline-info" />
				<a href="javascript:void(0);" onclick="history.go(-1);" class="btn btn-outline-info">Go Back</a>
			</div>
		</div>
	</form>
</div>

